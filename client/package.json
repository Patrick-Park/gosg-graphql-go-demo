{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "React Starter Kit with Webpack + Babel + Tape",
  "main": "index.js",
  "scripts": {
    "build-v1": "NODE_ENV=production webpack -p --config webpack.v1.production.config.js --progress --profile --colors",
    "dev-v1": "webpack --progress --profile --colors --watch --config webpack.v1.config.js",
    "build-v2": "NODE_ENV=production webpack -p --config webpack.v2.production.config.js --progress --profile --colors",
    "dev-v2": "webpack --progress --profile --colors --watch --config webpack.v2.config.js",
    "test": "./node_modules/babel-tape-runner/bin/babel-tape-runner tests/*.jsx | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sogko/react-starter.git"
  },
  "keywords": [
    "react",
    "babel",
    "webpack",
    "starter",
    "tape",
    "es6",
    "es2015"
  ],
  "author": "Hafiz Ismail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sogko/react-starter/issues"
  },
  "homepage": "https://github.com/sogko/react-starter#readme",
  "devDependencies": {
    "apollo-client": "^0.3.12",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-relay-plugin": "^0.8.1",
    "babel-tape-runner": "^2.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-apollo": "^0.3.5",
    "react-css-modules": "^3.7.6",
    "react-dom": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-relay": "^0.8.1",
    "react-router": "^2.4.1",
    "react-router-relay": "^0.13.2",
    "redux": "^3.5.2",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
